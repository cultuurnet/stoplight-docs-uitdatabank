{
  "title": "organizer-contactPoint-put",
  "type": "object",
  "properties": {
    "url": {
      "type": "array",
      "description": "An array of urls.",
      "items": {
        "type": "string",
        "pattern": "^http[s]?:\\/\\/",
        "example": "https://www.publiq.be"
      }
    },
    "email": {
      "type": "array",
      "description": "An array of email addresses.",
      "items": {
        "type": "string",
        "format": "email",
        "example": "info@publiq.be"
      }
    },
    "phone": {
      "type": "array",
      "description": "An array of phone numbers.",
      "items": {
        "type": "string",
        "example": "016 10 20 30",
        "minLength": 1
      }
    }
  }
}
{
  "title": "place.geo",
  "type": "object",
  "description": "Geocoordinates corresponding to the [address](./place-address.json) the place is located at. Calculated automatically by UiTdatabank.",
  "examples": [
    {
      "latitude": 50.8816177,
      "longitude": 4.7137986
    }
  ],
  "properties": {
    "latitude": {
      "type": "number",
      "format": "float",
      "example": 50.8816177,
      "description": "For example `50.8816177`",
      "minimum": -90,
      "maximum": 90
    },
    "longitude": {
      "type": "number",
      "description": "For example `4.7137986`",
      "format": "float",
      "example": 4.7137986,
      "minimum": -180,
      "maximum": 180
    }
  },
  "required": [
    "latitude",
    "longitude"
  ],
  "readOnly": true
}